 <html>
   <body>
     <h1>{{studio.name}}</h1>
     <hr>
     <img src="{{studio.image}}" alt="studio pic">
      <ul>
            {{#each studio.schedule}}
            <form method="post" action="/studio/views/book-time/{{../studio._id}}/{{this}}">
            <li>{{@key}}
            <button type="submit">{{this}}</button>
            </li>
            </form>
            {{/each}}
            <li>{{studio.address}}</li>
            <li>{{studio.cost}}</li>
            <li>{{studio.phoneNumber}}</li>
            <li>{{studio.email}}</li>
            <li><a href="/studio/review/{{studio._id}}">Leave a Review</a></li>
      </ul>
         <br>
        <hr>
    
          <a href="/studio"><h4>Back to Studios</h4></a>
   </body>
 </html>
