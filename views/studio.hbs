<head>
    <h1>Studios In Miami</h1>

        {{#each studio}}
            <h3>{{this.name}}</h3>
            <img src="{{this.image}}" alt="studio pic">
        <ul>
            <li>{{this.address}}</li>
            <li>{{this.cost}}</li>
            <li>{{this.availability}}</li>
            <li>{{this.phoneNumber}}</li>
            <li>{{this.email}}</li>
            <li>{{this.addOns}}</li>
        </ul>
        {{/each}}
</head>

